package JAVAAssignment;

public class Problem5 {

    public static void main(String[] args) {

        Problem5 pmm1 = new Problem5();
        pmm1.number();
    }

    public void number()
    {
        for (int i = 500; i <=1000; i++)
        {
            if(i==808)
            {
                System.out.println("Number Found as "+i);
            }
        }
    }
}
