package JAVAAssignment;



public class Problem24 {

    public int width;
    public int hight;
    public  int length;


    public static void main(String[] args) {
        Problem24 pr = new Problem24(5);
        System.out.println(pr.width);
        System.out.println(pr.hight);
        System.out.println(pr.length);
        System.out.println(pr.length+pr.width+pr.hight);
    }

    public Problem24(int width,int hight,int length)
    {
        this.width = width;
        this.length = length;
        this.hight = hight;
    }
    public Problem24(int width)
    {
        this(width,width,width);

    }
}
